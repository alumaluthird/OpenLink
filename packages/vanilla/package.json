{
  "name": "@openlink/vanilla",
  "version": "1.0.0",
  "description": "Vanilla JavaScript integration for OpenLink - works with any HTML site",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm,iife --dts --global-name OpenLink",
    "dev": "tsup src/index.ts --format cjs,esm,iife --dts --global-name OpenLink --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vanilla-js",
    "html",
    "solana",
    "wallet",
    "browser"
  ],
  "license": "MIT",
  "dependencies": {
    "@openlink/core": "^1.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}

