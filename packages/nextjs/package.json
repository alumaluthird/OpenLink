{
  "name": "@openlink/nextjs",
  "version": "1.0.0",
  "description": "Next.js integration for OpenLink",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.mjs",
      "require": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.tsx src/server.ts --format cjs,esm --dts",
    "dev": "tsup src/index.tsx src/server.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "nextjs",
    "solana",
    "wallet",
    "app-router",
    "pages-router"
  ],
  "license": "MIT",
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@openlink/core": "^1.0.0",
    "@openlink/react": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}

