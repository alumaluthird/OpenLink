{
  "name": "@openlink/db",
  "version": "1.0.0",
  "description": "Database migration and integration utilities for OpenLink",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "openlink-db": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "database",
    "migration",
    "sql",
    "mongodb",
    "prisma"
  ],
  "license": "MIT",
  "dependencies": {
    "@openlink/core": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}

